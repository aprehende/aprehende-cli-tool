import React from 'react';

{{#if withHook}}
const useHello = () => {
  const greet = () => {
    console.log('Hello, world!');
  };

  return { greet };
};

export default useHello;

{{else}}
const {{hookName}} = () => {
  const greet = () => {
    console.log('Hello, world!');
  };

  return { greet };
};

export default {{hookName}};
{{/if}}
